<bindings version="2.1" 
          xmlns="http://java.sun.com/xml/ns/jaxb"
          xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc"
          xmlns:phloc="http://www.phloc.com/ns/jaxb22/plugin" 
          xmlns:xs="http://www.w3.org/2001/XMLSchema"
          xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
          xsi:schemaLocation="http://java.sun.com/xml/ns/jaxb http://java.sun.com/xml/ns/jaxb/bindingschema_2_0.xsd"
          extensionBindingPrefixes="phloc">
  <globalBindings fixedAttributeAsConstantProperty="true" typesafeEnumMaxMembers="32000" />
  <bindings schemaLocation="../resources/schemas/Holiday.xsd" node="/xs:schema">
    <schemaBindings>
      <package name="com.phloc.holiday.config" />
    </schemaBindings>
  </bindings>
</bindings>
